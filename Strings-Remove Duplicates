
import java.util.*;
public class Main {
    public static void main(String args[]) {
        // Your Code Here
		Scanner scn =new Scanner(System.in);
		String str = scn.next();
		String ans = "";
		for(int i=0; i<str.length()-1;i++){
			if(str.charAt(i)!=str.charAt(i+1)){
				ans =ans+ str.charAt(i);
			}
		}
		System.out.println(ans+str.charAt(str.length()-1));
    }
}
